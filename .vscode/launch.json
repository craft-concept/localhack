{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/src/vscode"],
      "outFiles": ["${workspaceFolder}/.hack/build/src/vscode/**/*.mjs"]
    }
  ]
}
